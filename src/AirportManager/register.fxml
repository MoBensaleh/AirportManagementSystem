<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="491.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AirportManager.RegisterController">
   <children>
      <Label alignment="CENTER" layoutX="202.0" layoutY="26.0" prefHeight="23.0" prefWidth="155.0" text="ADD USERS">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="112.0" layoutY="93.0" text="ID Number" />
      <TextField fx:id="idNumberTextField" layoutX="112.0" layoutY="119.0" prefHeight="25.0" prefWidth="336.0" promptText="Please enter your ID number" />
      <Label layoutX="112.0" layoutY="160.0" prefHeight="17.0" prefWidth="66.0" text="Password" />
      <PasswordField fx:id="setPasswordField" layoutX="111.0" layoutY="186.0" prefHeight="27.0" prefWidth="336.0" promptText="Please enter your password" />
      <Label layoutX="111.0" layoutY="230.0" prefHeight="17.0" prefWidth="120.0" text="Confirm Password" />
      <PasswordField fx:id="confirmPasswordField" layoutX="112.0" layoutY="253.0" prefHeight="27.0" prefWidth="336.0" promptText="Retype password" />
      <Label layoutX="113.0" layoutY="300.0" text="Select User Type" />
      <ComboBox fx:id="selectionComboBox" layoutX="112.0" layoutY="323.0" prefHeight="27.0" prefWidth="336.0" promptText="Type of User" />
      <Button fx:id="registerButton" layoutX="112.0" layoutY="389.0" mnemonicParsing="false" onAction="#registerButtonOnAction" prefHeight="27.0" prefWidth="336.0" text="Register" />
      <Button fx:id="closeButton" layoutX="113.0" layoutY="429.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="27.0" prefWidth="336.0" text="Close" />
      <Label fx:id="messageLabel" alignment="CENTER" layoutY="64.0" prefHeight="18.0" prefWidth="559.0" textFill="#37e119">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="passMessageLabel" alignment="CENTER" layoutX="248.0" layoutY="230.0" prefHeight="18.0" prefWidth="199.0" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="errorMessageLabel" alignment="CENTER" layoutX="2.0" layoutY="364.0" prefHeight="17.0" prefWidth="559.0" textFill="#e81414" />
   </children>
</AnchorPane>
